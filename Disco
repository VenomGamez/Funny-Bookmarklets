javascript:(function(){
    var disco = document.createElement('div');
    disco.style.width = '100%';
    disco.style.height = '100%';
    disco.style.position = 'fixed';
    disco.style.top = '0';
    disco.style.left = '0';
    disco.style.pointerEvents = 'none';
    disco.style.background = 'url(https://media.giphy.com/media/Zcu4c9Y3zT52Y/giphy.gif)';
    document.body.appendChild(disco);
})();
